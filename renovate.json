{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["config:recommended"],
  "customManagers": [
    {
      "customType": "regex",
      "fileMatch": ["README\\.md$"],
      "matchStrings": [
        "go install (?<depName>[\\S]+?)@(?<currentValue>v\\S+)"
      ],
      "datasourceTemplate": "go"
    }
  ]
}
